Algoritmo Ej1 
	Definir matriz como entero
	Dimension matriz[4,4]
	para i <- 1 Hasta 4 Hacer
		para j <- 1 Hasta 4 Hacer
			matriz[i,j]=0
		FinPara
	FinPara
	para i <- 1 Hasta 4 Hacer
		para j <- 1 Hasta 4 Hacer
			Escribir Sin Saltar matriz[i,j], " "
			
		FinPara
		Escribir " "
	FinPara

	
FinAlgoritmo
